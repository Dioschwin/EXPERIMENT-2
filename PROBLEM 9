// Dioschwin N. Sumo
// Section 1-12
// Problem 9

#include <iostream>

using namespace std;

int main()
{
	int x;
	int y;
	int row;
	int column;
	
	cout << "How many rows: ";
	cin >> row;
	
	cout << "How many columns: ";
	cin >> column;
	
	for (x = 1; x <= row; x++)
	{
		for (y = 1; y <= column; y++)
		{
		cout << "* ";
		}
		cout << endl;
	}
	
	return 0;
	
	
}
