// Dioschwin N. Sumo
// Section 1-12
// Problem 6

#include<iostream>
using namespace std;

int main()
{
	int ctr, x=0, y=1, result;
	cout<<"Fibonacci numbers: "<<endl;
	for (ctr = 0;ctr<=21;ctr++ )
   {
      if (ctr <= 1 )
         result = ctr;
      else
      {
         result= x + y;
         x = y;
         y = result;
      }
      cout<<result<<",";
   }
 
return 0;		
}
